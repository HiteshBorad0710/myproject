                                                                class="btn btn-warning">Deactive</a>
                                                        <!-- <a href="/sliderDeactive/<%= val._id %>" name="Deactive" class="form-check form-switch"><input class="form-check-input" type="checkbox" role="switch" checked></a> -->

                                                            <% } %>
                                                </td>
                                                <td>

                                                     <!-- <a href="/edit/<%= val._id %>" class="btn btn-success">Edit</a> -->
                                                     <a href="/timeslotEdit/<%= val._id %>" class="btn btn-success"><i class="bi bi-pencil-square"></i></a>

                                                    <!-- <a href="/delete/<%= val._id %>" class="btn btn-danger">Delete</a> || -->
                                                    <a href="/timeslotDelete/<%= val._id %>" class="btn btn-danger"><i class="bi bi-trash"></i></a> 


                                                </td>
                                            </tr>
        
                                            <% }) %>
